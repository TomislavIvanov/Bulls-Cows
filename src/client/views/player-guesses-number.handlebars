<div class="container">
	<div class="bs-docs-section">
		<div class="row">
			<!-- logo.nunjucks -->
			<div class="col-lg-4">
				<div class="page-header">
					<div class="col-lg-4">
						<div class="col-lg-8 col-md-7 col-sm-6">
							<a href="/">
								<img src="/imgs/cows_and_bulls.jpg">
							</a>
						</div>
					</div>
				</div>
			</div>
			<div class="col-lg-4">
				<div class="page-header">
					<p class="lead ">Guess number</p>
				</div>
				<div class="bs-component">
					<div id="successMsgLbl" class="panel panel-success hidden">
						<div class="panel-heading">
							<h3 class="panel-title"></h3>
						</div>
						<div class="panel-body">
							You guessed the number !!!
						</div>
					</div>
					<div id="errorMsgLbl" class="alert alert-dismissible alert-danger hidden">
						<p>Please enter valid number.</p>
					</div>
				</div>
			</div>
			<div class="col-lg-4">
			</div>
		</div>
		<div class="row">
			<div class="col-lg-4">
				<!-- notes.nunjucks -->
				<div class="col-lg-8 col-md-7 col-sm-6">
					<div class="list-group">
						<a href="#" class="list-group-item">
							<h4 class="list-group-item-heading">Notes:</h4>
							<p class="list-group-item-text">1. All digits are different.</p>
							<p class="list-group-item-text">2. Number can not start with zero.</p>
							<p class="list-group-item-text">3. If some of the digits match the right possitions, they are marked as bulls.</p>
							<p class="list-group-item-text">4. If some of the digits match but they are on other possitions they are marked as cows..</p>
						</a>
					</div>
				</div>
			</div>
			<div class="col-lg-4">

				<div class="bs-component">
					<div class="input-group">
						<input id="txtNumber" maxlength="4" type="text" class="form-control" id="inputNumber" placeholder="Enter suggestion" disabled>
						<span class="input-group-btn">
							    <button id="btnSubmitNum" class="btn btn-default hidden" type="button">Submit number</button>
                            </span>
						<span class="input-group-btn">
								<button id="btnStartGame" class="btn btn-default" type="button">Start new game</button>
							</span>
					</div>
				</div>
				<div class="bs-component">
				</div>
				<div class="bs-component">
					<table id='informationLog' class="table table-striped table-hover hidden">
						<thead>
							<tr>
								<th>#</th>
								<th>Number</th>
								<th>Bulls</th>
								<th>Cows</th>
							</tr>
						</thead>
						<tbody>

						</tbody>
					</table>
				</div>
			</div>
			<div class="col-lg-4">
			</div>
		</div>
	</div>
</div>
<!-- include bower componets -->
<!-- bowerComponents.nunjucks -->
<script src="/bower_components/socket.io-client/socket.io.js"></script>
<script src="/bower_components/jquery/dist/jquery.min.js"></script>
<script src="/bower_components/bluebird/js/browser/bluebird.min.js"></script>
<script src="/bower_components/jquery-ui/jquery-ui.min.js"></script>
<script src="/scripts/player_guesses_number.js"></script>